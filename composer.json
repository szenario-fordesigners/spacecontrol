{
	"name": "szenario/craft-spacecontrol",
	"description": "Webspace Monitoring",
	"type": "craft-plugin",
	"license": "mit",
	"version": "0.0.1",
	"keywords": [
		"monitoring",
		"diskspace"
	],
	"support": {
		"email": "support@szenario.design",
		"issues": "https://github.com/thomasbendl/craft-spacecontrol/issues?state=open",
		"source": "https://github.com/thomasbendl/craft-spacecontrol",
		"docs": "https://github.com/thomasbendl/craft-spacecontrol",
		"rss": "https://github.com/thomasbendl/craft-spacecontrol/releases.atom"
	},
	"require": {
		"php": ">=8.2.5",
		"craftcms/cms": "^4.4.8"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"szenario\\craftspacecontrol\\": "src/"
		}
	},
	"extra": {
		"handle": "spacecontrol",
		"name": "spacecontrol",
		"developer": "szenario.design",
		"documentationUrl": "https://github.com/thomasbendl/craft-spacecontrol",
		"class": "szenario\\craftspacecontrol\\SpaceControl"
	},
	"scripts": {
		"check-cs": "ecs check --ansi",
		"fix-cs": "ecs check --ansi --fix",
		"phpstan": "phpstan --memory-limit=1G"
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.0.2"
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
